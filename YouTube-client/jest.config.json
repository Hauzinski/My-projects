{
  "bail": 1,
  "clearMocks": true,
  "collectCoverageFrom": ["src/**/*.{js,jsx,ts,tsx}", "!**/declarations.d.*", "!src/App.*", "!src/index.*"],
  "coveragePathIgnorePatterns": ["src/models"],
  "coverageThreshold": {
    "global": {
      "lines": 90
    }
  },
  "moduleFileExtensions": ["tsx", "ts", "json", "node", "jsx", "js", "mjs", "cjs"],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./__mocks__/fileMock.js",
    "\\.(css|less|scss)$": "identity-obj-proxy",
    "^@/(.*)$": "<rootDir>/src/$1"
  },
  "setupFilesAfterEnv": ["./config/setupTest.ts"],
  "slowTestThreshold": 8,
  "testEnvironment": "jsdom",
  "transform": {
    "\\.[jt]sx?$": "babel-jest"
  },
  "verbose": true
}
